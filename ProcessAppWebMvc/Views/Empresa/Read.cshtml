@model IEnumerable<Entity_Layer.EMPRESA>

@{
    ViewBag.Title = "Read";
}

<h2>Read</h2>

<p>
    @Html.ActionLink("Crear Empresa", "Insert")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RUT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NOMBRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIRECCION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CORREO_CONTACTO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TELEFONO_CONTACTO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ESTADO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.ID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.RUT)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NOMBRE)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DIRECCION)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.CORREO_CONTACTO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TELEFONO_CONTACTO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ESTADO)
    </td>
    <td>
        @Html.ActionLink("Editar", "Update", new { ID = item.ID }) |
        @*@Html.ActionLink("Details", "Details", new { id=item.ID })*@ |
        @Html.ActionLink("Eliminar", "Delete", new { ID = item.ID })
    </td>
</tr>
