<style>
    body {
        padding-top: 0px;
        padding-bottom: 0px;
    }


    #nav_processsa_logo {
        width: 115px;
    }

    #nav_processsa_logo_2 {
        width: 115px;
        padding-top: 20px;
    }

    @@media (max-width: 992px) {
        #nav_processsa_logo_2 {
            display: none;
        }
    }

    .btn-success,
    .btn-info {
        width: 49%;
    }

    @@media (min-width: 400px) {
        .select-mul {
            min-width: 400px;
        }
    }


    @@media (max-width: 394px) {
        .select-mul {
            min-width: 350px;
        }
    }

    @@media (min-width: 992px) {
        .select-mul {
            width: 500px;
        }

        .card {
            margin-left: 5px;
        }
    }

    .btn-eliminar {
        width: 100%;
    }
